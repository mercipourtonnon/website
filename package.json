{
  "name": "mercipourtonnon",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.16",
    "@types/bun": "latest",
    "tailwindcss": "^4.1.16"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  },
  "scripts": {
    "start": "bun index.html charte.html",
    "render": "bun run generated/render.js",
    "css": "bunx @tailwindcss/cli -i ./input.css -o ./output.css --minify",
    "css:watch": "bunx @tailwindcss/cli -i ./input.css -o ./output.css --watch",
    "build": "bun run render && bun run css",
    "dev": "bun run render && bun run css:watch"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}
